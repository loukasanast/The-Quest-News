<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>org.thequestnews.Index</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/data.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Comment</servlet-name>
        <servlet-class>org.thequestnews.Comment</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Comment</servlet-name>
        <url-pattern>/comment.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>org.thequestnews.Upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/upload.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Publish</servlet-name>
        <servlet-class>org.thequestnews.Publish</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Publish</servlet-name>
        <url-pattern>/publish.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>org.thequestnews.Search</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/search.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Counter</servlet-name>
        <servlet-class>org.thequestnews.Counter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Counter</servlet-name>
        <url-pattern>/counter.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>index.jsp</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/home/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/politics/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/business/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/culture/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/sport/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/tech/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/celebs/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/article/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index.jsp</servlet-name>
        <url-pattern>/publish/*</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>